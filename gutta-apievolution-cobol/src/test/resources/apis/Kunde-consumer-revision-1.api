api kunde.consumer {

    record NameRec as Name {
    	string(64)   Vorname
	    string(1)    NameFiller1 as filler
	    string(32)   Nachname
	}
	
    record ProviderOutput as Kunde {
        numeric(8)   KundenNr
	    NameRec      Name
	    string(10)   Geburtsdatum                   
		string(1)    Geschlecht
		string(16)   Telefonnummer                    
		string(512)  Mailadresse
    }
    
}